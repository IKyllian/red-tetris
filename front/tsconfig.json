{
  "compilerOptions": {
    // "types": ["vitest/globals", "@testing-library/jest-dom", "@testing-library/jest-dom/vitest"],
    "types": ["vitest/globals"],
    "target": "ES2020",
    "moduleResolution": "node",
    "module": "es2020",
    "lib": ["DOM", "DOM.Iterable", "ES2020"],
    "allowSyntheticDefaultImports": true,
    "baseUrl": "./",
    "jsx": "react-jsx",
    "skipLibCheck": true,
    "paths": {
      "front/*": ["src/*"],
    },
    "allowJs": true,
    "noEmit": true,
    "rootDir": "./"
  },
  "include": ["src", "__tests__/vitest.setup.ts"]
}